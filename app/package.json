{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview",
		"prepare": "husky || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint -c eslint.config.fast.js --quiet src/",
		"lint:fix": "eslint -c eslint.config.fast.js --fix src/",
		"lint:full": "prettier --check . && eslint --cache .",
		"lint:ci": "prettier --check . && eslint --cache .",
		"lint:errors": "eslint --cache --max-warnings 0 src/",
		"lint:components": "eslint src/lib/components/",
		"lint:routes": "eslint src/routes/",
		"lint:utils": "eslint src/lib/utils/ src/lib/services/",
		"lint:unused": "eslint --fix . --ext .ts,.js,.svelte --rule 'unused-imports/no-unused-imports: error'",
		"lint:staged": "lint-staged",
		"test:unit": "vitest",
		"test": "pnpm run test:unit -- --run && pnpm run test:e2e",
		"test:integration": "playwright test integration-real-api.test.ts",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:smoke": "playwright test smoke.test.ts",
		"test:e2e:critical": "playwright test critical-user-journeys.test.ts",
		"convert:swagger": "swagger2openapi ../docs/swagger/swagger.json -o ../docs/swagger/openapi.json",
		"generate:api-types": "pnpm dlx swagger2openapi ../docs/swagger/swagger.json --outfile ../docs/swagger/openapi-converted.json && pnpm dlx openapi-typescript ../docs/swagger/openapi-converted.json --output src/lib/types/api.d.ts",
		"generate:api-types:all": "pnpm run convert:swagger && pnpm run generate:api-types",
		"test:all": "pnpm run test:unit -- --run && pnpm run test:integration && pnpm run test:e2e",
		"commitlint": "commitlint --edit",
		"hooks:install": "husky",
		"hooks:uninstall": "husky uninstall"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@internationalized/date": "^3.8.0",
		"@lucide/svelte": "^0.482.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tanstack/svelte-query-devtools": "^5.75.7",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/d3-scale": "^4.0.9",
		"@types/node": "^22.15.21",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^1.4.7",
		"clsx": "^2.1.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"eslint-plugin-unused-imports": "^4.1.4",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"jsdom": "^26.0.0",
		"layerchart": "2.0.0-next.10",
		"lint-staged": "^16.1.0",
		"msw": "^2.8.4",
		"openapi-typescript": "^7.8.0",
		"postcss": "^8.5.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"swagger2openapi": "^7.0.8",
		"tailwind-merge": "^3.2.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"ts-prune": "^0.10.3",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vitest": "^3.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@tanstack/svelte-form": "^1.11.1",
		"@tanstack/svelte-query": "^5.75.7",
		"@tanstack/table-core": "^8.21.3",
		"@tanstack/zod-form-adapter": "^0.42.1",
		"@vite-pwa/sveltekit": "^1.0.0",
		"cron-parser": "^5.1.1",
		"cronstrue": "^2.61.0",
		"d3-array": "^3.2.4",
		"d3-interpolate": "^3.0.1",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0",
		"d3-time": "^3.1.0",
		"d3-time-format": "^4.1.0",
		"date-fns": "^4.1.0",
		"dexie": "^4.0.11",
		"lucide-svelte": "^0.509.0",
		"maplibre-gl": "^5.5.0",
		"svelte-maplibre": "^1.0.0",
		"svelte-persisted-store": "^0.12.0",
		"svelte-sonner": "^0.3.28",
		"svelte-tel-input": "^3.6.0",
		"workbox-precaching": "^7.3.0",
		"workbox-window": "^7.3.0",
		"zod": "^3.24.4"
	},
	"msw": {
		"workerDirectory": [
			"static"
		]
	},
	"packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
